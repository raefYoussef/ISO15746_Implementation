	<!-- Choose which Advance Control Process Module -->
	<xs:element name="VendorAttributesAPCModule">
		<xs:complexType>
			<xs:choice>

				<!-- Choose: MPC Control -->
				<xs:element name="MatrixMPC">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="PredictionHorizon" type="xs:integer"/>
							<xs:element name="ControlHorizon" type="xs:integer"/>
							<xs:element name="VendorAttributesMPC"/>
							<xs:element name="TimeUnits" type="xs:string"/>
							<!--  Type? Description:Specific points in the ControlHorizon  where MV moves are calculated-->
							<xs:element name="MoveBlocks"/>

							<!-- Sets of Pairs : Relationship between MC/CV or DV/CV-->
							<xs:element name="ModelPairs" maxOccurs="unbounded">

								<xs:complexType>
									<xs:sequence>
										<!-- Indicated if model is self-regulating relationship or integrating relationship -->
										<xs:element name="IsIntegrator" type="xs:string"/>
										<xs:element name="ControlVariable" type="xs:string"/>
										<xs:element name="ManipulatedVariableOrDisturbanceVariable"
											type="xs:string"/>
										<!-- Set of parameters required to express the model there are many different types of models but only two  examples
										are shown. A Parametric Model Parameter Set includes the prameters needed to fefine a continous-time transfer function. An FIR Model
										Parameter Set includes the parameters needed to define a finite impulse response (FIR) Model -->
										<xs:element name="ModelParameterSet" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>

												<!-- ParametricModeSet -->
												<xs:element name="ParametricModelSet">
												<xs:complexType>

												<xs:sequence>
												<xs:element name="Order" type="xs:integer"/>
												<xs:element name="Deadtime" type="xs:float"/>
												<xs:element name="Lead" type="xs:float"/>
												<xs:element name="Gain" type="xs:float"/>
												<xs:element name="Tau1" type="xs:float"/>
												<xs:element name="Tau2" type="xs:float"/>
												<xs:element name="Zeta" type="xs:float"/>
												</xs:sequence>

												</xs:complexType>

												</xs:element>
												<!-- ParametricModeSet End -->

												<!-- FIRParametricSet -->
												<xs:element name="FIRModelParametrtSet">
												<xs:complexType>
												<xs:sequence>
												<xs:element name="Gain" type="xs:float"/>
												<xs:element name="NumCoefficients" type="xs:float"/>
												<xs:element name="Coefficient" type="xs:float"/>
												</xs:sequence>
												</xs:complexType>
												</xs:element>
												<!-- FIRParametricSet End -->

												</xs:sequence>
											</xs:complexType>
										</xs:element>

									</xs:sequence>
								</xs:complexType>

							</xs:element>
							<!--  Sets of Pairs End -->
						</xs:sequence>
					</xs:complexType>

				</xs:element>
				<!-- Choose: MPC Control End -->

				<!--  Choose: Expert System Control -->
				<xs:element name="ExpertSystem">
					<xs:complexType>

						<xs:sequence>
							<xs:element name="Rules" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<!-- The rule itself is in a vendor-specific format this could be a simple "If temperature
										X is greater than 500 then open the cooling water valve by 5 percent" or it could be a more
										complex combination of if then statements-->
										<xs:element name="Rule" type="xs:string"/>
										<!-- Execution Conditions  some execute on a time frequency pther times others when
										informed that new data is available-->
										<xs:element name="InitiationCriteria" type="xs:string"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>

					</xs:complexType>
				</xs:element>
				<!--  Choose: Expert System Control End -->

				<!--  Choose: Transition Procedure Control -->
				<xs:element name="TransitionProcedure">

					<xs:complexType>
						<xs:sequence>
							<!-- the initial product -->
							<xs:element name="FromProduct" type="xs:string"/>
							<!-- the element thats transferred into -->
							<xs:element name="ToProduct" type="xs:string"/>
							<!-- the current state of the executing procedure -->
							<xs:element name="ProcedureStatus" type="xs:string"/>
							<!--A set of actions that the transition procedure follows -->
							<xs:element name="Steps" maxOccurs="unbounded">

								<xs:complexType>
									<xs:sequence>
										<!-- function the step like "turn on pump" -->
										<xs:element name="Actions" type="xs:string"/>
										<!-- signal or logic defining when step is complete by signal ot logic
											ex:"FeedTankLevel>20% AND PumpingRunning=TRUE-->
										<xs:element name="CompletionCriteria" type="xs:string"/>
									</xs:sequence>
								</xs:complexType>

							</xs:element>
						</xs:sequence>

					</xs:complexType>

				</xs:element>
				<!--  Choose: Transition Procedure Control End -->

			</xs:choice>


		</xs:complexType>
	</xs:element>

	<xs:element name="VendorAttributesSoftSensorModule">
		<xs:complexType>
			<xs:sequence>
				<!-- -->
				<xs:element name="LabAnalyzerValue" type="xs:double"/>
				<xs:element name="LabAnalyzerTimeStamp" type="xs:dateTimeStamp"/>
				<xs:element name="CalculationStatus" type="xs:double"/>

				<xs:choice>
					<!-- type? -->
					<!-- how to write equations
					based off one or more equatons typically based off on physical relationships like materials or energy balance-->
					<xs:element name="Equation">
						<xs:complexType>
							<xs:sequence>
								<xs:element maxOccurs="unbounded" minOccurs="1" name="EquationSet">
									<xs:complexType>
										<xs:sequence>
