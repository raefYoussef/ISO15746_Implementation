				<xs:element name="VariableAttributeSet">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Name" type="xs:string"/>
							<!-- type? -->
							<xs:element name="VariableID" type="xs:string"/>
							<xs:element name="DataType" type="xs:string"/>
							<xs:element name="ModuleType" type="xs:string"/>
							<!-- type? -->
							<xs:element name="Source" type="xs:string"/>
							<xs:element name="EngineeringUnits" type="xs:string"/>
							<xs:element name="VariableActive" type="xs:boolean"/>
							<!-- type? -->
							<xs:element name="VariableStatusEvent">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Event" maxOccurs="1" minOccurs="0">

											<xs:complexType>
												<xs:sequence>
												<!-- type? A reference to the object that generated the event -->
												<xs:element name="Source" type="xs:string"/>
												<xs:element name="Type" type="xs:string"/>
												<!-- type? Urgency of case -->
												<xs:element name="Severity" type="xs:int"/>
												<xs:element name="Time" type="xs:time"/>
												<!-- Process Events or System Events -->
												<xs:element name="EventCategory" type="xs:string"/>
												<xs:element name="VendorAttributeEvents"/>
												<!-- type? -->
												<xs:element name="TrackingEvent"/>
												<xs:element name="ConditionEvent">
												<xs:complexType>
												<xs:sequence>
												<xs:element form="qualified" name="ConditionName"
												type="xs:Name"/>
												<xs:element name="NewState" type="xs:string"/>
												</xs:sequence>
												</xs:complexType>
												</xs:element>
												</xs:sequence>
											</xs:complexType>

										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="VendorAttributesVariableType"/>


							<xs:element ref="iso:BehaviorSpecificType"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>

				<xs:element name="VariableValueSet">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ProcessValue" type="xs:string"/>
							<xs:element name="Quality" type="xs:float"/>
							<xs:element name="TimeStamp" type="xs:dateTimeStamp"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>

			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="ControlledVariable">
		<xs:sequence>
			<!-- type? adjustment to the model based on plant feedback-->
			<xs:element name="Bias" type="xs:float"/>
			<xs:element name="Target" type="xs:double"/>
			<xs:element name="MinimumSoftLimit" type="xs:double"/>
			<xs:element name="MaximumSoftLimit" type="xs:double"/>
			<xs:element name="Price" type="xs:double"/>
			<!-- Scaling used to determine the relative importance of a deviation from target or limits -->
			<xs:element name="OptimizationScale" type="xs:float"/>
			<xs:element maxOccurs="unbounded" minOccurs="1" ref="iso:PerformanceStatistics"/>
			<xs:element name="VendorAttributesVariable"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ManipulatedVariable">
		<xs:sequence>
			<!-- type? Set point of source -->
			<xs:element name="Setpoint" type="xs:string"/>
			<!-- Source in Remote Description: flag indicating source is remote -->
			<xs:element name="SourceInRemote" type="xs:double"/>
			<xs:element name="RateOfChangeLimit" type="xs:double"/>
			<xs:element name="MoveSuppression" type="xs:double"/>
			<xs:element name="MinimumHardLimit" type="xs:double"/>
			<xs:element name="MaximumHardLimit" type="xs:double"/>
			<xs:element name="Price" type="xs:double"/>
			<xs:element name="OptimizationScale" type="xs:float"/>
			<xs:element maxOccurs="unbounded" ref="iso:PerformanceStatistics"/>
			<xs:element name="VendorAttributesVariable"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="DisturbanceVariable">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" ref="iso:PerformanceStatistics"/>
			<xs:element name="VendorAttributesVariable"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="DependentVariable">
		<xs:sequence>
			<!-- type? -->
			<xs:element name="Bias" type="xs:float"/>
			<xs:element name="Target" type="xs:double"/>
			<xs:element name="MinimumLimit" type="xs:double"/>
			<xs:element name="MaximumLimit" type="xs:double"/>
			<xs:element name="Price" type="xs:double"/>
			<xs:element name="OptimizationScale" type="xs:float"/>
			<xs:element maxOccurs="unbounded" ref="iso:PerformanceStatistics"/>
			<xs:element name="VendorAttributesVariable"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="IndependantVariable">
		<xs:sequence>
			<!-- type? -->
			<xs:element name="Setpoint" type="xs:string"/>
			<xs:element name="MinimumLimit" type="xs:double"/>
			<xs:element name="MaximumLimit" type="xs:double"/>
			<xs:element name="Price" type="xs:double"/>
			<xs:element name="OptimizationScale" type="xs:float"/>
			<xs:element maxOccurs="unbounded" ref="iso:PerformanceStatistics"/>
			<xs:element name="VendorAttributesVariable"/>
		</xs:sequence>
	</xs:complexType>







</xs:schema>
